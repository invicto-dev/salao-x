{
  "name": "salao-x-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Backend para a plataforma Salao X",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:worker\"",
    "dev:server": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/workers/productImportWorker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/utils/seed.ts",
    "db:seed-prd": "node dist/utils/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.59.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.9",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "concurrently": "^9.2.1",
    "prisma": "^6.16.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
